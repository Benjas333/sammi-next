{
    "name": "sammi-websocket-types",
    "version": "1.1.0",
    "author": {
        "name": "Benjas333",
        "url": "https://github.com/Benjas333"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Benjas333/SAMMI-Next.git",
        "directory": "packages/sammi-websocket-types"
    },
    "module": "dist/SAMMIWebSocket.d.ts",
    "devDependencies": {
        "rimraf": "^6.1.2"
    },
    "description": "Type definitions for SAMMI Websocket",
    "files": [
        "dist/**/*",
        "types/**/*"
    ],
    "keywords": [
        "typescript",
        "sammi",
        "sammi-websocket",
        "types"
    ],
    "license": "MIT",
    "private": false,
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "update:git": "git fetch origin && git pull && bun run install-ci",
        "install-ci": "cd ../../vendor/sammi-websocket && npm ci && cd ../../packages/sammi-websocket-types",
        "clean": "rimraf dist",
        "build": "bun run update:git && bun run rebuild",
        "rebuild": "bun run clean && bun x tsc"
    },
    "types": "dist/SAMMIWebSocket.d.ts"
}
